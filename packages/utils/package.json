{
  "name": "@walletconnect/utils",
  "description": "Utilities for WalletConnect Protocol",
  "version": "2.0.0-beta.101",
  "author": "WalletConnect, Inc. <walletconnect.com>",
  "homepage": "https://github.com/walletconnect/walletconnect-monorepo/",
  "license": "Apache-2.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "wallet",
    "walletconnect"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "help": "tsc --help",
    "build:pre": "run-s clean",
    "build:types": "tsc",
    "build:source": "rollup --config rollup.config.js",
    "build": "run-s build:pre build:source build:types",
    "test:pre": "rm -rf ./test/test.db",
    "test:run": "vitest run --dir test",
    "test": "run-s test:pre test:run",
    "lint": "eslint -c '../../.eslintrc' --fix './src/**/*.ts'"
  },
  "dependencies": {
    "@stablelib/chacha20poly1305": "^1.0.1",
    "@stablelib/hkdf": "^1.0.1",
    "@stablelib/random": "^1.0.1",
    "@stablelib/sha256": "^1.0.1",
    "@stablelib/x25519": "^1.0.2",
    "@walletconnect/jsonrpc-utils": "1.0.3",
    "@walletconnect/logger": "^1.0.0",
    "@walletconnect/relay-api": "1.0.4",
    "@walletconnect/safe-json": "^1.0.0",
    "@walletconnect/time": "^1.0.1",
    "@walletconnect/types": "^2.0.0-beta.101",
    "@walletconnect/window-getters": "^1.0.0",
    "@walletconnect/window-metadata": "^1.0.0",
    "lodash.isequal": "4.5.0",
    "query-string": "^6.13.5",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash.isequal": "4.5.6"
  }
}
