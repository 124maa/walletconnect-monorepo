    "lint": "eslint -c '../../.eslintrc' --fix './src/**/*.ts'",
    "prettier": "prettier --check '{src,test}/**/*.{js,ts,jsx,tsx}'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.9",
    "fast-text-encoding": "1.0.4",
    "react-native-get-random-values": "1.8.0",
    "events": "3.3.0"
  }
}
