{
  "name": "@walletconnect/types",
  "description": "Typings for WalletConnect Protocol",
  "version": "2.0.0-beta.101",
  "author": "WalletConnect, Inc. <walletconnect.com>",
  "homepage": "https://github.com/walletconnect/walletconnect-monorepo/",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "browser": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "unpkg": "dist/umd/index.min.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "wallet",
    "walletconnect"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:pre": "run-s clean",
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "build:umd": "webpack",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "build": "run-s build:pre build:cjs build:esm build:umd",
    "watch": "tsc -p tsconfig.json --watch",
    "lint": "eslint -c '../../.eslintrc' --fix './src/**/*.ts'"
  },
  "dependencies": {
    "@walletconnect/events": "^1.0.0",
    "@walletconnect/heartbeat": "^1.0.0",
    "@walletconnect/jsonrpc-types": "1.0.1",
    "@walletconnect/keyvaluestorage": "^1.0.0",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0"
  }
}
